\" + d.value} value={d.value}>\n      {d.text}\n    </Option>\n  ));\n  return (\n    <Select\n      showSearch\n      value={value}\n      placeholder={props.placeholder}\n      style={props.style}\n      defaultActiveFirstOption={false}\n      showArrow={false}\n      filterOption={false}\n      onSearch={handleSearch}\n      onChange={handleChange}\n      notFoundContent={null}\n    >\n      <OptGroup label=\"最近使用\" key=\"used\">\n        {usedOptions}\n      </OptGroup>\n      <OptGroup label=\"输入搜索\" key=\"search\">\n        {options}\n      </OptGroup>\n    </Select>\n  );\n}\n\nexport default SearchInput;\n",["38","39"],{"ruleId":"40","replacedBy":"41"},{"ruleId":"42","replacedBy":"43"},{"ruleId":"44","severity":1,"message":"45","line":4,"column":8,"nodeType":"46","messageId":"47","endLine":4,"endColumn":16},{"ruleId":"48","severity":1,"message":"49","line":58,"column":6,"nodeType":"50","endLine":58,"endColumn":8,"suggestions":"51"},{"ruleId":"52","severity":1,"message":"53","line":76,"column":23,"nodeType":"54","messageId":"55","endLine":76,"endColumn":25},{"ruleId":"56","severity":1,"message":"57","line":154,"column":7,"nodeType":"58","endLine":156,"endColumn":10},{"ruleId":"44","severity":1,"message":"45","line":2,"column":8,"nodeType":"46","messageId":"47","endLine":2,"endColumn":16},{"ruleId":"48","severity":1,"message":"59","line":33,"column":5,"nodeType":"50","endLine":33,"endColumn":17,"suggestions":"60"},{"ruleId":"44","severity":1,"message":"45","line":2,"column":8,"nodeType":"46","messageId":"47","endLine":2,"endColumn":16},{"ruleId":"52","severity":1,"message":"53","line":51,"column":15,"nodeType":"54","messageId":"55","endLine":51,"endColumn":17},{"ruleId":"52","severity":1,"message":"61","line":56,"column":45,"nodeType":"54","messageId":"55","endLine":56,"endColumn":47},{"ruleId":"48","severity":1,"message":"59","line":79,"column":6,"nodeType":"50","endLine":79,"endColumn":8,"suggestions":"62"},{"ruleId":"40","replacedBy":"63"},{"ruleId":"42","r